{
  "name": "bookbag",
  "version": "1.0.0",
  "description": "Node.js/React.js-based research assistant application.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CatLibWilk/bookbag.git"
  },
  "author": "Andrew Wilk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CatLibWilk/bookbag/issues"
  },
  "homepage": "https://github.com/CatLibWilk/bookbag#readme",
  "dependencies": {
    "ajv": "^6.5.4",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
